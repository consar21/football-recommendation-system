     find_results_for_winger(_, _, Position, SubPosition, PlayerHeight, PlayerPhysicality, PlayerRate, PlayerPrice, _, _, _, _, _, _, _, Pace, _, Results),

    (Results = [] -> write("Sorry, based on the given criteria no suitable player found! Please change your criteria"); writeln("System recommends the following Players:"),nl, print_list(Results)), end.


find_results_for_winger(_, _, Position, SubPosition, PlayerHeight, PlayerPhysicality, PlayerRate, PlayerPrice, _, _, _, _, _, _, _, Pace, _, Results) :-
