print_list([]).
print_list([(Name, Team, Price)|T]) :-
    format("Name: ~w,  Team: ~w,  Price: ~w EUR~n", [Name, Team, Price]),
    print_list(T).


within_range(Value, [Min, Max]) :-
    Value >= Min,
    Value =< Max.

